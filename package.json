{
  "name": "formulist",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    
    "backend": "cd backend && python -m uvicorn main:app --reload --port 8000",
    "backend:install": "pip install -r backend/requirements.txt",
    
    "install:all": "npm install && npm run backend:install",
    "start": "concurrently \"npm run backend\" \"npm run dev\"",
    
    "clean": "rm -rf dist node_modules backend/__pycache__"
  },
  "dependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.9.4"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.3.4",
    "vite": "^6.0.0",
    "concurrently": "^9.0.0"
  }
}